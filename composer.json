{
  "name": "drupal/intercept",
  "type": "drupal-module",
  "description": "Drupal InterCEPT module.",
  "keywords": [
    "Drupal"
  ],
  "license": "MIT",
  "homepage": "https://www.drupal.org/project/intercept",
  "minimum-stability": "dev",
  "support": {
    "issues": "https://www.drupal.org/project/issues/intercept",
    "source": "http://cgit.drupalcode.org/intercept"
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "asset-packagist": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  },
  "require": {
    "drupal/address": "^1.3",
    "drupal/consumers": "^1.0.0-beta3",
    "drupal/consumer_image_styles": "^2.0.0-rc5",
    "drupal/duration_field": "^1.1",
    "drupal/entity_browser": "^2.0-alpha2",
    "drupal/features": "^3.5",
    "drupal/field_group": "^1.0",
    "drupal/paragraphs": "^1.2",
    "drupal/date_recur": "^1.0.alpha5",
    "drupal/simple_oauth": "^3.5",
    "drupal/focal_point": "1.0-beta6",
    "drupal/flag": "^4.0@alpha",
    "drupal/csv_serialization": "^1.0"
  },
  "require-dev": {
    "bower-asset/dropzone": "^5.1"
  },
  "extra": {
    "installer-paths": {
      "docroot/libraries/{$name}": [
        "type:bower-asset",
        "type:npm-asset"
      ]
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ]
  }
}
